{
  "name": "neulab-release",
  "version": "0.1.1",
  "description": "analysis of spontaneous alliterations",
  "private": true,
  "main": "public/electron.js",
  "author": "Aleksei Drobkov abkommem@gmail.com",
  "contributors": [
      {
        "name": "Aleksei Drobkov",
        "email": "abkommem@gmail.com",
        "url": "https://github.com/AlexeyDrobkovStrikesBack/neulab-release-0.1.0"
      },
     {
       "name": "Alim Bashirzade",
       "email": "alim.bashirzade95@mail.ru"
     }
  ],
  "homepage": "https://github.com/AlexeyDrobkovStrikesBack/neulab-release-0.1.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "electron-builder",
    "clean": "rm -rf dist",
    "electron-start": "electron .",
    "release": "npm run build && npm run electron-start",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron-start\"",
    "start-forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "neulabId",
    "productName": "Neulab App",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    }
  },
  "extends": null,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "concurrently": "^8.2.1",
    "electron": "^27.0.0",
    "electron-is-dev": "^2.0.0"
  }
}
